<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Adds name generation files to Gondor -->
  <!-- Check the root directory/Languages/English/strings -->
  <!--   to find files with names for the faction -->
  <RulePackDef>
    <defName>LotRMB_GondorMaleNamer</defName>
    <rulePack>
      <rulesStrings>
        <li>name-&gt;[pack]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>pack</keyword>
          <path>NameBanks/Names_GondorMale</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  <RulePackDef>
    <defName>LotRMB_GondorFemaleNamer</defName>
    <rulePack>
      <rulesStrings>
        <li>name-&gt;[pack]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>pack</keyword>
          <path>NameBanks/Names_GondorFemale</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
</Defs>